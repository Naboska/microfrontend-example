{
  "parser": "babel-eslint",
  "plugins": [
    "eslint-plugin-prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "arrowParens": "avoid",
        "trailingComma": "es5",
        "tabWidth": 2,
        "singleQuote": true,
        "printWidth": 100
      }
    ]
  }
}
